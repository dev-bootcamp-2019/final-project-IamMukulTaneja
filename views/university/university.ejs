<h1>University Portal</h1>

<fieldset>
<legend> Award Degree</legend>
<form action="/university/award" method="POST" enctype="multipart/form-data">
    <label for="EnrollNum">University Enrollment Number(UEN):</label>
    <input type = "text" id="EnrollNum" name="EnrollNum">

    <br>
    <br>
    <br>

    <label for="Degree">Upload Degree: </label>
    <input type="file" id="Degree" name="Degree">

    <br>
    <br>
    <br>

    <button>Award Degree</button>
</form>
</fieldset>
<br>
<fieldset>
    <legend>Degree Status</legend>
    <form>
        <label for="EnrollNum">University Enrollment Number(UEN):</label>
        <input type = "text" id="EnrollNum" name="EnrollNum">
    
    
        <button>Get Status</button>
    </form>
</fieldset>
<br>
<fieldset>
    <legend>Revoke Status</legend>
    <form>
        <label for="EnrollNum">University Enrollment Number(UEN): </label>
        <input type = "text" id="EnrollNum" name="EnrollNum">
        <button>Get Status</button>
    </form>
</fieldset>



